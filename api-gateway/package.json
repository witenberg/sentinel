    "nestjs-zod": "^5.1.1",
    "pg": "^8.13.0",
    "pino": "^10.3.1",
    "pino-http": "^11.0.0",
    "redis": "^5.11.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.3",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
